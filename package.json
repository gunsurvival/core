{
	"type": "module",
	"name": "@gunsurvival/core",
	"version": "1.0.0",
	"description": "Core library for gunsurvival game",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"tsc-watch": "tsc --watch",
		"watch": "nodemon --exec \"npm run build && npm start\" --watch src -e ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": ">=5.43.0",
		"@typescript-eslint/parser": ">=5.43.0",
		"eslint": ">=8.0.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-typescript": "^0.55.1",
		"nodemon": "^2.0.20",
		"typescript": ">=4.4"
	},
	"dependencies": {
		"@colyseus/schema": "^1.0.45",
		"detect-collisions": "^6.6.11",
		"sat": "^0.9.0"
	},
	"exports": {
		".": "./src/index.ts",
		"./effect/*": "./src/effect/*",
		"./entity/*": "./src/entity/*",
		"./player/*": "./src/player/*",
		"./util/*": "./src/util/*",
		"./world/*": "./dist/world/*",
		"./effect": "./src/effect/index.ts",
		"./entity": "./src/entity/index.ts",
		"./player": "./src/player/index.ts",
		"./util": "./src/util/index.ts",
		"./world": "./dist/world/index.js",
		"./stats": "./src/stats.ts",
		"./types": "./src/types.ts"
	}
